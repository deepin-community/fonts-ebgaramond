lookup v_alternates {
    sub v from [ v.init v.alt v.initalt ];
    sub v.init from [ v.init v.initalt v.initalt ];
    sub u.LATinit from [ v.init v.alt v.initalt ];
} v_alternates;

feature cv05 {
    lookup v_alternates;
} cv05;

