lookup cyrillic_DLLj {
    sub [ uni0409 uni0414 uni041B ] by [ uni0409.alt uni0414.alt uni041B.alt ];
} cyrillic_DLLj;

feature ss01{
    lookup cyrillic_DLLj;
} ss01;

lookup historic_u {
    ignore sub [ @Letters quoteright ] u';
    sub u' by u.LAT;
    sub [ @Letters quoteright ] v' by v.LATmedi;
    sub U' by U.LAT;
#    sub [ U.sc u.sc ]' by [ U.LATsc u.LATsc ];
} historic_u;

feature ss02 {
    lookup historic_u;
} ss02;

lookup ss03 {
    sub [ v w ] by [ v.alt w.alt ];
} ss03;

feature ss03 {
    lookup ss03;
} ss03;

lookup alternate_h {
    sub [ h k hcircumflex uni01E9 uni021F uni1E23 uni1E27 uni1E31 ] by [ h.01 k.01 hcircumflex.01 uni01E9.01 uni021F.01 uni1E23.01 uni1E27.01 uni1E31.01 ];
} alternate_h;

feature ss07 {
#    lookup CCMP_Precomp;
    lookup alternate_h;
} ss07;

lookup longs_replacement {
     sub s' [ @letters hyphen uni200D ] by longs;
} longs_replacement;

feature cv01 {
 lookup longs_replacement;
} cv01;

lookup at_alternates {
