} cv47;

lookup VW_alts {
    sub [ V W V.sc W.sc v.sc w.sc ] by [V.01 W.01 V.01sc W.01sc v.01sc w.01sc];
} VW_alts;

feature ss10 {
    lookup VW_alts;
} ss10;

lookup cv82_decomp {
    sub dcaron by d uni030C.alt;
    sub lcaron by l uni030C.alt;
    sub tcaron by t uni030C.alt;
    sub Lcaron by L uni030C.altcap;
} cv82_decomp;

lookup cv82 {
    sub uni030C.alt by uni030C.alt1;
    sub uni030C.altcap by uni030C.altcap1;
} cv82;

feature cv82 {
    lookup cv82_decomp;
    lookup cv82;
} cv82;

lookup salt_lig {
    sub a s by a_s;
    sub e s by e_s;
    sub eacute s by eacute_s;
    sub i s by i_s;
    sub u s by u_s;
} salt_lig;

feature salt {
    lookup salt_lig;
} salt;
